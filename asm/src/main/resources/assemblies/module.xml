<?xml version="1.0"?>

<assembly>
	<id>module</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/</outputDirectory>
			<useProjectArtifact>true</useProjectArtifact>
			<scope>runtime</scope>
			<unpack>true</unpack>
		</dependencySet>
	</dependencySets>
	<fileSets>
		<fileSet>
			<directory>${project.build.directory}/main/java</directory>
			<outputDirectory>/</outputDirectory>
			<includes>
				<include>module-info.class</include>
				<include>META-INF/product.card</include>
				<include>META-INF/product.info</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>
